{
  "name": "plsql-outline",
  "displayName": "PL/SQL Outline",
  "description": "Professional PL/SQL code outline view for VS Code",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/example/plsql-outline"
  },
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "plsqlOutlineContainer",
          "title": "PL/SQL Outline",
          "icon": "$(symbol-class)"
        }
      ]
    },
    "views": {
      "plsqlOutlineContainer": [
        {
          "id": "plsqlOutline",
          "name": "PL/SQL Outline"
        }
      ]
    },
    "commands": [
      {
        "command": "plsqlOutline.expandAll",
        "title": "Expand All",
        "icon": "$(expand-all)"
      },
      {
        "command": "plsqlOutline.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "plsqlOutline.openSettings",
        "title": "Settings",
        "icon": "$(settings-gear)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "plsqlOutline.expandAll",
          "when": "view == plsqlOutline",
          "group": "navigation@1"
        },
        {
          "command": "plsqlOutline.refresh",
          "when": "view == plsqlOutline",
          "group": "navigation@2"
        },
        {
          "command": "plsqlOutline.openSettings",
          "when": "view == plsqlOutline",
          "group": "navigation@3"
        }
      ]
    },
    "configuration": {
      "title": "PL/SQL Outline",
      "properties": {
        "plsqlOutline.fileExtensions": {
          "type": "array",
          "default": [".sql", ".pks", ".pkb", ".fcn", ".prc", ".typ", ".vw"],
          "description": "File extensions to be parsed as PL/SQL files",
          "items": {
            "type": "string"
          }
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "release": "standard-version && vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "standard-version": "^9.5.0",
    "typescript": "^5.0.4",
    "vsce": "^2.15.0"
  }
}
